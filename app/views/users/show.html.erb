<%= stylesheet_link_tag    "users" %>
<%= javascript_include_tag "users" %>
 <%= csrf_meta_tag %>

<h1>User Profile</h1>

<p>
  <b>Name:</b>
  <%= best_in_place @user, :username %>
</p>
<p>
  <b>Email:</b>
  <%= best_in_place @user, :email %>
</p>

<%= link_to 'Edit Profile', edit_user_path(@user) %>
